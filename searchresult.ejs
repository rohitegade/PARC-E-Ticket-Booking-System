                        </div>
                        <div id="<%=rt+500%>" class="panel-collapse collapse">
                            <div class="panel-body">
                                <% for (var c=0;c<data1.length;c++) {%>
                                <% if(rt==data1[c].rtno) { %>
                                <table><tr><%=data1[c].tm%></tr> </table>
